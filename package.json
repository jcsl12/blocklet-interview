{
  "name": "inteview-demo",
  "version": "1.0.0",
  "description": "demo",
  "main": "index.js",
  "scripts": {
    "pre": "cnpm install || yarn --registry https://registry.npm.taobao.org || npm install --registry https://registry.npm.taobao.org ",
    "test": "cross-env ENV_NAME=stage NODE_ENV=development node webpack/build.js",
    "start": "npm run run:dev",
    "run:dev": "cross-env ENV_NAME=dev NODE_ENV=development webpack serve --config webpack/webpack.dev.config.js",
    "build:dev": "cross-env ENV_NAME=dev NODE_ENV=production webpack --config webpack/webpack.prod.config.js",
    "zip:dev": "cross-env ENV_NAME=dev NODE_ENV=production webpack --config webpack/webpack.prod.config.js & node webpack/bin/zip_dist.js",
    "run:stage": "cross-env ENV_NAME=stage NODE_ENV=development webpack serve --config webpack/webpack.dev.config.js",
    "build:stage": "cross-env ENV_NAME=stage NODE_ENV=production webpack --config webpack/webpack.prod.config.js",
    "run:prod": "cross-env ENV_NAME=prod NODE_ENV=development webpack serve --config webpack/webpack.dev.config.js",
    "build:prod": "cross-env ENV_NAME=prod NODE_ENV=production webpack --config webpack/webpack.prod.config.js"
  },
  "author": "daguanren",
  "license": "None",
